{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM",
      "dom"
    ],
    "esModuleInterop": false,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "alwaysStrict": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "strictBindCallApply": true,
    "baseUrl": "../nuxt config typescript",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": ["@nuxt/types", "@nuxtjs/axios", "@types/node"]
  },
  "exclude": ["node_modules", ".nuxt", "dist", "static"]
}
